<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gudla.tgtourism">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:icon="@drawable/tg_tourism_icon"
            android:label="Telangana Tourism">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.gudla.tgtourism.MainActivity" />
        </activity>
        <activity android:name=".DestinationsActivity" />
        <activity
            android:name=".DivineDestinationActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".HeritageActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".NatureActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".WildLifeActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".AdventureActivity"
            android:configChanges="orientation|screenSize" />
        <activity
            android:name=".RegionsActivity"
            android:configChanges="orientation|screenSize" />
        <activity android:name=".ContactUsActivity" />
        <activity
            android:name=".hyd.BrilaActivity"
            android:label="@string/title_activity_brila"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyA5thEPrcS6so_nJNSfBTrUPlHMVFzIXwc" />

        <activity
            android:name=".hyd.JagannathActivity"
            android:label="@string/title_activity_jagannath"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".hyd.HyderabadActivity" />
        <activity android:name=".khm.KhammamActivity" />
        <activity android:name=".mbn.MahabubnagarActivity" />
        <activity android:name=".nld.NalgondaActivity" />
        <activity android:name=".WarangalActivity" />
        <activity android:name=".adb.AdilabadActivity" />
        <activity android:name=".nzb.NizamabadActivity" />
        <activity android:name=".knr.KarimnagarActivity" />
        <activity android:name=".mdk.MedakActivity" />
        <activity android:name=".rr.RangareddyActivity"></activity>
    </application>

</manifest>